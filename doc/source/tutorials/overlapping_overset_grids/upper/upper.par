[GENERAL]
polynomialOrder = 7
stopAt = endTime
endTime  = 10
variableDT = yes
dt = targetCFL = 0.4 + initial=1e-1
timeStepper = bdf2
checkpointControl = steps
checkpointInterval = 20
udf = "../hillnn.udf"

[PROBLEMTYPE]
equation = navierstokes

[VELOCITY]
residualTol = 1e-8
density = 1
viscosity = -100

[NEKNEK]
boundaryEXTOrder = 2

[MESH]
file = "upper.re2"

[CASEDATA]
interpolated_bc_id = 3
